<launch>
	<include file="$(find gazebo_ros)/launch/empty_world.launch" >
		<arg name="use_sim_time" value="true"/>
		<arg name="gui" value="true"/>
	</include>
	
	<node pkg="gazebo_ros" type="spawn_model" name="gazebo_spawn"
		args="-file $(find testpkg0)/urdf/ball.urdf -urdf -x 3 -y 0 -z 0 -model MYROBOT3">
	</node>
	
	<node pkg = "testpkg0" type = "node2" name = "node2">
		<param name = "x0" type = "double" value = "3.0"/>
		<param name = "y0" type = "double" value = "0.0"/>
		<param name = "z0" type = "double" value = "0.0"/>
		<param name = "ball_vel" type = "double" value = "3.0"/>
	</node>
	
	<param name="robot_description" textfile="$(find testpkg0)/urdf/ball.urdf" />
	
	
</launch>
